
server {

    server_name test.oms.mooving;

    location /static/ {
        root /home/ubuntu/gunicorn-project/;
    }

    location /media/ {
        root /home/ubuntu/gunicorn-project/;
    }

    location = /favicon.ico { access_log off; log_not_found off; }

    location / {
        include proxy_params;
        proxy_pass http://unix:/tmp/gunicorn.sock;
    }

}

